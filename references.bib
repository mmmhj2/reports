@inbook{Tollmien1961,
  author    = {Tollmien, Walter
               and Schlichting, Hermann
               and G{\"o}rtler, Henry
               and Riegels, F. W.},
  editor    = {Riegels, F. W.},
  title     = {{\"U}ber Fl{\"u}ssigkeitsbewegung bei sehr kleiner Reibung},
  booktitle = {Ludwig Prandtl Gesammelte Abhandlungen: zur angewandten Mechanik, Hydro- und Aerodynamik},
  year      = {1961},
  publisher = {Springer Berlin Heidelberg},
  address   = {Berlin, Heidelberg},
  pages     = {575--584},
  abstract  = {In der klassischen Hydrodynamik wird vorwiegend die Bewegung der reibungslosen Fl{\"u}ssigkeit behandelt. Von der reibenden Fl{\"u}ssigkeit besitzt man die Differentialgleichung der Bewegung, deren Ansatz durch physikalische Beobachtungen wohl best{\"a}tigt ist. An L{\"o}sungen dieser Differentialgleichung hat man au{\ss}er eindimensionalen Problemen, wie sie u. a. von LordRayleigh1 gegeben wurden, nur solche, bei denen die Tr{\"a}gheit der Fl{\"u}ssigkeit vernachl{\"a}ssigt ist oder wenigstens keine Rolle spielt. Das zwei-und dreidimensionale Problem mit Ber{\"u}cksichtigung von Reibung und Tr{\"a}gheit harrt noch der L{\"o}sung. Der Grund hierf{\"u}r liegt wohl in den unangenehmen Eigenschaften der Differentialgleichung.},
  isbn      = {978-3-662-11836-8},
  doi       = {10.1007/978-3-662-11836-8_43},
  url       = {https://doi.org/10.1007/978-3-662-11836-8_43}
}


